<Application x:Class="JediTournamentWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:JediTournamentWPF"
             StartupUri="MainWindow.xaml">
             <!--StartupUri="MainWindow.xaml">-->
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Themes -->
                <ResourceDictionary Source="Themes/ButtonsDictionary.xaml"/>
                <ResourceDictionary Source="Themes/ComboBoxDictionary.xaml"/>
                <ResourceDictionary Source="Themes/TextDictionary.xaml"/>
                <!-- Converter -->
                <ResourceDictionary Source="Resources/Converter/ConverterDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
